/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
	Or(a = a, b = b, out = or1);
	Nand(a = a, b = b, out = nand2);
	And(a = or1, b = nand2, out = out);
}